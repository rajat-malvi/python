   "source": [
    "lst=[1,2,0,4,6,5,3,9]\n",
    "# to find odd number list we create a empty list\n",
    "odd_list=[]\n",
    "for i in lst:\n",
    "    # use if condition to check number is odd or not. \n",
    "    if i%2!=0:\n",
    "        odd_list.append(i)\n",
    "print(odd_list)"
