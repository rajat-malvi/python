   "source": [
    "lst=[2,4,3,1,67,6,0]\n",
    "# to find even number list we create a empty list\n",
    "even_list=[]\n",
    "for i in lst:\n",
    "    # use if condition to check number is even or not. \n",
    "    if i%2==0:\n",
    "        even_list.append(i)\n",
    "print(even_list)"
